#!/bin/sh
set -e

#DEBHELPER#

case $1 in
    remove)
        conflicts="/usr/share/desktop-base/kf5-settings/kdeglobals /usr/share/applications/mimeapps.list"
        for i in $conflicts
        do
            dpkg-divert --package radxa-system-config-rockchip --rename --remove "$i"
        done
        ;;
esac

exit 0